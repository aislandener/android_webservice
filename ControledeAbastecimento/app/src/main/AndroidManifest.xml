<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lab2.controledeabastecimento" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".TelaPricipal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.CadastrarAbastecimento"
            android:label="@string/title_activity_cadastrar_abastecimento" >
        </activity>
        <activity
            android:name=".view.VisualizarAbastecimento"
            android:label="@string/title_activity_visualizar_abastecimento" >
        </activity>
        <activity
            android:name=".view.TotalAbastecimento"
            android:label="@string/title_activity_total_abastecimento" >
        </activity>
    </application>

</manifest>
